{
  "name": "frontendconf-contentful",
  "version": "1.0.0",
  "repository": {
    "url": "https://git.responsive.ch/responsive/frontendconf-contentful.git",
    "type": "git"
  },
  "author": "Thomas Jaggi <thomas@responsive.ch>",
  "license": "UNLICENSED",
  "engines": {
    "node": "8.1.0"
  },
  "dependencies": {
    "array-shuffle": "^1.0.1",
    "autoprefixer": "^7.1.1",
    "breakpoint-sass": "^2.7.1",
    "contentful": "^3.8.1",
    "cssnano": "^3.10.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "fecha": "^2.3.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash.clonedeep": "^4.5.0",
    "lru-cache": "^4.1.1",
    "marked": "^0.3.6",
    "next": "^2.4.4",
    "node-sass": "^4.5.3",
    "path-match": "^1.2.4",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-ga": "^2.2.0",
    "restify": "^4.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "susy": "^2.2.12"
  },
  "devDependencies": {
    "eslint": "^3.10.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "husky": "^0.14.3",
    "now": "^6.2.5",
    "prettier-standard": "^5.1.0",
    "regenerator-runtime": "^0.10.5"
  },
  "scripts": {
    "build": "next build",
    "predeploy": "PREDEPLOY=true next build && (if [ -f .next/static/css/default.css ]; then mv .next/static/css/default.css static/css/; fi)",
    "start": "node server.js",
    "deploy": "now -e NODE_ENV=production",
    "alias": "now alias set",
    "logs": "now logs www.frontendconf.ch",
    "logs-dev": "now logs dev.frontendconf.ch",
    "lint": "eslint .",
    "precommit": "echo $(git diff --name-only --diff-filter=ACM | grep '\\.\\(js\\|scss\\)\\?$' | tr '\n' ' ') | xargs npm run prettier-cmd",
    "prettier": "echo \"*.{js,scss}\" \"!(.next)/*.{js,scss}\" | xargs npm run prettier-cmd",
    "prettier-cmd": "prettier-standard --print-width 100",
    "update-dependencies": "npx npm-check-updates -u -a"
  }
}
